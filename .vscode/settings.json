{
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "cpp": "g++ -std=c++17 $fileName -o $fileNameWithoutExt.exe && $fileNameWithoutExt.exe < input.txt > output.txt",
        "c": "gcc $fileName -o $fileNameWithoutExt.exe && $fileNameWithoutExt.exe < input.txt > output.txt"
    }
}


